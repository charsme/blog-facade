{
  "name": "blog-facade",
  "version": "1.0.0",
  "description": "My amazing Nuxt.js project",
  "author": "Charis",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "cross-env TEST=unit nyc -c --clean --skip-empty ava ./test/specs/**/*.spec.js --verbose --color",
    "test:unit:watch": "cross-env TEST=unit nyc -c --clean --skip-empty ava ./test/specs/**/*.spec.js --verbose --color --watch",
    "test:e2e": "cross-env TEST=e2e nightwatch ./test/e2e/*.js",
    "test:ava": "cross-env TEST=unit ava ./test/specs/**/*.spec.js --verbose --color",
    "test": "npm run test:unit & npm run test:e2e",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@tryghost/content-api": "^1.2.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "nuxt": "^2.0.0",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "^1.0.0",
    "@nuxtjs/eslint-module": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "chromedriver": "^75.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": ">=13.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": "^22.10.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "nightwatch": "^1.1.13",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.0.5"
  }
}
